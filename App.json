  "expo": {
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "TWO needs access to your Camera"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/JejuGothic.ttf"
          ]
        }
      ]
    ],
    "android": {
      "permissions": [
        "android.permission.CAMERA"
      ],
